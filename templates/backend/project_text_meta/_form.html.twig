{{ form_start(form) }}
<div class="row">
    <div class="form-group col-md-4">
        <label class="control-label">KEY</label>
        <h4>{{ project_text_metum.metaKey }}</h4>
    </div>
    <div class="form-group col-md-4{% if form.memo.vars.errors|length %} has-error{% endif %}">
        {{ form_label(form.memo) }}
        {{ form_errors(form.memo) }}
        {{ form_widget(form.memo) }}
        {{ form_help(form.memo) }}
    </div>
    <div class="form-group col-md-4{% if form.textMeta.vars.errors|length %} has-error{% endif %}">
        {{ form_label(form.textMeta) }}
        {{ form_errors(form.textMeta) }}
        {{ form_widget(form.textMeta) }}
        {{ form_help(form.textMeta) }}
    </div>
</div>
{{ form_widget(form) }}
<a class="btn btn-danger" href="{{ path('project_text_meta_index') }}">{{ 'Back to list'|trans }}</a>
<button class="btn btn-primary">{{ button_label|default('Save') }}</button>
{{ form_end(form) }}